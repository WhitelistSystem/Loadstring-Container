Window_1:AddToggle({text = 'No Cooldown Toggle',state = false,callback = function(s)
        local r = require(game:service('ReplicatedStorage').suitData)
        r = r.suitData
        for a=2,40 do
            pcall(function()
                r['Mark'..a].cooldownMult = s and math.huge or 1
            end)
        end
end})

Window_1:AddToggle({text = 'GodMode',state = false,callback = function(s)
local player = game:GetService("Players").LocalPlayer
if player and player.Character then 
if player.Character:FindFirstChild("Humaniod") then 
for i,v2 in pairs(player.Character.Suit:GetChildren())do 
if v2:FindFirstChild("Suit") then 
player.Backpack.suitControl.Assets.Events.punchHit:FireServer(v2.Union, -math.huge)
end
end
end
end
end})

Window_1:AddToggle({text = 'Auto Send Suit',state = false,callback = function(s)
    local LSend; 
function loop()
    while LSend == true do
        wait()
    pcall(function()
    for i,v in pairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer then
        local player = game:GetService('Players').LocalPlayer
        if player and player.Character then
        if player.Character:FindFirstChild('ForceField') then
            
        game:GetService("Players").LocalPlayer.Backpack.ironMan.Assets.Events.callSuit:FireServer("Mark33", tick(), 0)
        end
        end

            end
        end
    end)
    end
end
LSend = true
loop()
end})

Window_1:AddToggle({text = 'Auto Unbreakable Suit',state = false,callback = function(s)
game.Players.LocalPlayer.Character.Suit.Status.damageTaken:Destroy()
wait()
local LSnap; 
function loopsnap()
    while LSnap == true do
        wait()
    pcall(function()
    for i,v in pairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer then
        local player = game:GetService('Players').LocalPlayer
        if player and player.Character then
        if player.Character:FindFirstChild('Suit') then
            wait(.6)
            wait(6)
            wait(4)
            wait(2)
        game.Players.LocalPlayer.Character.Suit.Status.damageTaken:Destroy()
        end
end

            end
        end
    end)
    end
end
LSnap = true
loopsnap()
end})

Window_1:AddBind({text = "Fast Reset Suit", key = "R_Control", save = true, callback = function()

local UserInputService = game:GetService("UserInputService")
local function GetInput(input, gpe)
if input.KeyCode == Enum.KeyCode.RightControl then 
    
game:GetService("Players").LocalPlayer.Backpack.suitControl.Assets.Events.Eject:FireServer()

wait(1)

game:GetService("Players").LocalPlayer.Backpack.ironMan.Assets.Events.removeSuit:FireServer()

local player = game:GetService('Players').LocalPlayer
if player and player.Character then
if player.Character:FindFirstChild('Suit') then
game.Players.LocalPlayer.Character.Suit:Destroy()
wait()

game:GetService("Players").LocalPlayer.Backpack.ironMan.Assets.Events.callSuit:FireServer("Mark33", tick(), 0)
end
end
end
end
UserInputService.InputBegan:Connect(GetInput)
end})

